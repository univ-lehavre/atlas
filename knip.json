{
  "$schema": "https://unpkg.com/knip@5/schema.json",
  "workspaces": {
    ".": {
      "project": ["*.{js,ts}"]
    },
    "packages/crf": {
      "project": ["src/**/*.ts"]
    },
    "packages/redcap": {
      "project": ["analyzer/**/*.ts", "tests/**/*.ts"]
    },
    "packages/eslint-config": {
      "project": ["*.js"]
    },
    "packages/typescript-config": {
      "project": ["*.json"]
    },
    "apps/*": {
      "project": ["src/**/*.ts"]
    }
  },
  "ignore": [
    "**/dist/**",
    "**/.svelte-kit/**",
    "**/build/**",
    "**/redcap-source/**"
  ],
  "ignoreDependencies": ["tsx"],
  "ignoreBinaries": ["jq", "docker:build", "docker:down", "uvx"]
}
