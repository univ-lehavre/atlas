{
  "$schema": "https://typedoc.org/schema.json",
  "entryPoints": ["packages/crf", "packages/net"],
  "entryPointStrategy": "packages",
  "out": "docs/api",
  "plugin": ["typedoc-plugin-markdown"],
  "readme": "none",
  "excludePrivate": true,
  "excludeInternal": true,
  "excludeExternals": true,
  "entryFileName": "index",
  "indexFormat": "table",
  "hidePageHeader": true,
  "hideBreadcrumbs": true
}
