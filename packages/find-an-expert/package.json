{
  "name": "@univ-lehavre/atlas-find-an-expert",
  "version": "0.5.1",
  "type": "module",
  "scripts": {
    "dev": "vite dev",
    "build": "vite build",
    "preview": "vite preview",
    "prepare": "svelte-kit sync || echo ''",
    "typecheck": "tsc --noEmit",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "format": "prettier --write --ignore-path ../../.prettierignore .",
    "format:check": "prettier --check --ignore-path ../../.prettierignore .",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test:unit": "vitest",
    "test": "pnpm run test:unit -- --run",
    "test:coverage": "vitest run --coverage",
    "knip": "knip",
    "cpd": "jscpd src"
  },
  "devDependencies": {
    "@sveltejs/adapter-node": "^5.5.1",
    "@sveltejs/kit": "^2.49.5",
    "@sveltejs/vite-plugin-svelte": "^6.2.1",
    "@tailwindcss/vite": "^4.1.18",
    "@types/node": "^25.0.10",
    "@types/swagger-ui-dist": "^3.30.6",
    "@univ-lehavre/atlas-shared-config": "workspace:*",
    "@vitest/coverage-v8": "^4.0.18",
    "eslint": "^9.39.2",
    "jscpd": "^4.0.7",
    "jsdom": "^27.4.0",
    "knip": "^5.81.0",
    "prettier": "^3.8.1",
    "prettier-plugin-svelte": "^3.4.0",
    "svelte": "^5.46.4",
    "svelte-check": "^4.3.4",
    "tailwindcss": "^4.1.18",
    "typescript": "^5.9.3",
    "vite": "^7.3.0",
    "vite-plugin-static-copy": "^3.2.0",
    "vitest": "^4.0.18",
    "vitest-axe": "^0.1.0"
  },
  "dependencies": {
    "@iconify/svelte": "^5.2.1",
    "@univ-lehavre/atlas-appwrite": "workspace:*",
    "@univ-lehavre/atlas-errors": "workspace:*",
    "@univ-lehavre/atlas-logos": "workspace:*",
    "@univ-lehavre/atlas-validators": "workspace:*",
    "node-appwrite": "^21.1.0",
    "simple-git": "^3.30.0",
    "swagger-ui-dist": "^5.31.0",
    "zod": "^4.3.5"
  }
}
